<div id='sign-up-form'>
  <div class="row">
    <h3 class="col-sm-7 col-sm-push-3" style="margin-bottom: 30px; margin-top: -10px; padding-left: 0px;"><b>Happy <%= Date.today.strftime("%A") %>!</b></h3>
  </div>
  <div class="row" style="padding-left: 0px; margin-top: -5px; margin-bottom: -7px;">
    <%= link_to image_tag("linkedin-login-button.png", width: 432, height: 60), user_linkedin_omniauth_authorize_path, class: "col-sm-push-0 col-sm-6"  %>
  </div>
  <br>
  <div class="row" style="padding-left: 14px;">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= f.hidden_field :user_type, value: params[:type] %>
      <%= devise_error_messages! %>



      <div class="form-group">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true, :class => 'form-control input-lg' %>
      </div>

      <div class="form-group">
        <%= f.label :password %>
        <%= f.password_field :password, autocomplete: "off", :class => 'form-control input-lg' %>
      </div>

      <div class="form-group">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off", :class => 'form-control input-lg' %>
      </div>

      <div class="actions">
        <%= f.submit "Sign up", :class => 'btn btn-success btn-block btn-lg' %>
      </div>
    <% end %>
  </div>
</div>